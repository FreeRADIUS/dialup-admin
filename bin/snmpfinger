#!/usr/bin/perl

$SNMPWALK="/usr/bin/snmpwalk";

$walk =`$SNMPWALK $ARGV[0] public .iso.org.dod.internet.private.enterprises.9.2.9.2.1.18`;
$walk.=`$SNMPWALK $ARGV[0] public .iso.org.dod.internet.private.enterprises.9.9.27.1.1.3.1.7`;

while($walk=~/\"(\w+?)\"/g){
 $user=lc($1);
 if($out) {
  $out=$out.",'$user'";
 }else{
  $out="'$user'";
 }
}
print "$out\n";
