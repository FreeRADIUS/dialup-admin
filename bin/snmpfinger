#!/usr/bin/perl

$SNMPWALK="/usr/bin/snmpwalk";
$host=shift;
$comm=shift || 'public';

$walk =`$SNMPWALK $host $comm .iso.org.dod.internet.private.enterprises.9.2.9.2.1.18`;
$walk.=`$SNMPWALK $host $comm .iso.org.dod.internet.private.enterprises.9.9.27.1.1.3.1.7`;

while($walk=~/\"([\w\-]+?)\"/g){
 $user=lc($1);
 if($out) {
  $out=$out.",'$user'";
 }else{
  $out="'$user'";
 }
}
print "$out\n";
